{
  "compilerOptions": {
    "target": "es6",             
    "module": "commonjs",        
    "strict": true,               
    "esModuleInterop": true,     
    "skipLibCheck": true,       
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-native"
  },
  "include": ["*.ts", "*.tsx"],
  "exclude": ["node_modules"]
}
